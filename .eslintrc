{
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "rules": {
    "eol-last": "off",
    "brace-style": "off",
    "comma-dangle": "off",
    "comma-spacing": "off",
    "eqeqeq": "off",
    "indent": ["error", "tab"],
    "key-spacing": "off",
    "keyword-spacing": "off",
    "max-len": "off",
    "no-ex-assign": "off",
    "no-extra-boolean-cast": "off",
    "no-multi-spaces": "off",
    "no-throw-literal": "off",
    "no-unreachable": "off",
    "radix": "off",
    "quote-props": "off",
    "quotes": ["error", "double"],
    "space-before-function-paren": "off",
    "space-in-parens": "off",
    "space-infix-ops": "off",
    "space-unary-ops": "off",
    "spaced-comment": "off",
    "no-trailing-spaces": ["error"],
    // "linebreak-style": ["error", "unix"],
    "semi": ["error", "always"]
  },
  
  // "parser": "babel-eslint",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  }
}
